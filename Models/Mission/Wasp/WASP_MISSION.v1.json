{
  "title": "WASP_MISSION",
  "type": "object",
  "description": "The Wasp mission data",
  "properties": {
    "name": {
      "type": "string",
      "description": "Task name",
      "example": "WASP_Mission_1"
    },
    "point": {
      "$ref": "../../geo/GeoPoint.v1.json"
    },
    "waspAzimuth": {
      "type": "number",
      "description": "Task azimuth",
      "minimum": 0,
      "maximum": 359.99,
      "example": 0
    }
  },
  "required": [
    "name",
    "point",
    "waspAzimuth"
  ]
}
